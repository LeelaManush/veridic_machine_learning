    "def fizz_buzz(inp):\n",
    "    if inp%3==0 & inp%5==0:\n",
    "        return \"FizzBuzz\"\n",
    "    elif inp%3==0:\n",
    "        return \"Fizz\"\n",
    "    elif inp%5==0:\n",
    "        return \"Buzz\"\n",
    "    else:\n",
    "        return inp\n",
    "fiz_inp=int(input(\"enter the number:\"))\n",
    "fizz_buzz(fiz_inp)"
   ]
